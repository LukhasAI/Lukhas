{
  "module": "symbolic.core.neuro_bridge",
  "version": "1.0.0",
  "type": "neuro_symbolic_bridge",
  "matriz_compatible": true,
  "description": "Bridge between quantum visual symbols and MATRIZ cognitive architecture",

  "capabilities": {
    "sends": [
      {
        "signal": "scene_graph_updated",
        "schema": "SceneGraphUpdate",
        "frequency": "on_change",
        "latency_target_ms": 30,
        "description": "Scene graph structure updated with new objects or relationships"
      },
      {
        "signal": "global_workspace_broadcast",
        "schema": "WorkspaceBroadcast",
        "frequency": "on_symbol_process",
        "latency_target_ms": 20,
        "description": "Visual symbol broadcast to global workspace"
      },
      {
        "signal": "perception_value_assigned",
        "schema": "PerceptionValue",
        "frequency": "on_assignment",
        "latency_target_ms": 15,
        "description": "Consciousness-aware value assigned to element"
      },
      {
        "signal": "emotional_coupling_created",
        "schema": "EmotionalCoupling",
        "frequency": "on_coupling",
        "latency_target_ms": 25,
        "description": "Visual symbol coupled with emotional state"
      },
      {
        "signal": "matriz_node_emitted",
        "schema": "MatrizNode",
        "frequency": "continuous",
        "latency_target_ms": 5,
        "description": "MATRIZ format node emission"
      }
    ],

    "receives": [
      {
        "signal": "process_visual_symbol",
        "schema": "ProcessSymbolRequest",
        "handler": "process_visual_symbol",
        "required": true,
        "description": "Process symbol through neuro-symbolic pipeline"
      },
      {
        "signal": "generate_scene_graph",
        "schema": "SceneGraphRequest",
        "handler": "generate_scene_graph",
        "required": false,
        "description": "Generate complete scene graph from symbols"
      },
      {
        "signal": "broadcast_to_workspace",
        "schema": "BroadcastRequest",
        "handler": "broadcast",
        "required": false,
        "description": "Broadcast symbol to global workspace"
      },
      {
        "signal": "couple_emotion",
        "schema": "EmotionCouplingRequest",
        "handler": "couple_emotion",
        "required": false,
        "description": "Couple symbol with emotional state"
      }
    ]
  },

  "main_classes": [
    {
      "name": "NeuroSymbolicBridge",
      "description": "Main bridge between visual and cognitive",
      "methods": [
        "process_visual_symbol",
        "generate_scene_graph",
        "broadcast",
        "couple_emotion",
        "to_matriz_node"
      ]
    },
    {
      "name": "SceneGraph",
      "description": "Structured scene representation",
      "properties": [
        "objects",
        "attributes",
        "relationships"
      ],
      "methods": [
        "add_object",
        "add_relationship"
      ]
    },
    {
      "name": "PerceptionValueField",
      "description": "Consciousness-aware values",
      "properties": [
        "values",
        "emotional_coupling"
      ],
      "methods": [
        "assign_value"
      ]
    },
    {
      "name": "GlobalWorkspace",
      "description": "Broadcast mechanism",
      "properties": [
        "broadcast_queue",
        "subscribers"
      ],
      "methods": [
        "broadcast"
      ]
    },
    {
      "name": "EmotionalCoupling",
      "description": "Emotion-symbol linkage",
      "properties": [
        "symbol_emotions",
        "resonance_patterns"
      ],
      "methods": [
        "couple"
      ]
    }
  ],

  "bridge_properties": {
    "max_scene_objects": 10000,
    "max_relationships": 50000,
    "max_broadcast_queue": 1000,
    "broadcast_retention_ms": 5000,
    "emotion_dimensions": ["valence", "arousal"],
    "perception_value_range": [0.0, 1.0],
    "matriz_emission_rate": 100
  },

  "dependencies": [
    "symbolic.core.visual_symbol",
    "numpy",
    "uuid",
    "time"
  ],

  "performance": {
    "max_latency_ms": 50,
    "memory_limit_mb": 200,
    "cpu_cores": 2,
    "scene_updates_per_second": 30,
    "broadcasts_per_second": 100,
    "matriz_emissions_per_second": 1000
  },

  "constellation_integration": {
    "stars": ["vision", "memory", "identity", "bio"],
    "primary_star": "vision",
    "validates": true,
    "global_workspace_enabled": true
  },

  "data_schemas": {
    "SceneGraphUpdate": {
      "scene_id": "string",
      "objects_added": "array",
      "objects_removed": "array",
      "relationships_added": "array",
      "relationships_removed": "array",
      "timestamp": "integer"
    },
    "WorkspaceBroadcast": {
      "broadcast_id": "string",
      "symbol_id": "string",
      "symbol_data": "object",
      "matriz_node": "object",
      "subscribers": "array",
      "timestamp": "integer"
    },
    "PerceptionValue": {
      "element_id": "string",
      "value": "float",
      "emotional_valence": "float",
      "emotional_arousal": "float",
      "confidence": "float"
    },
    "EmotionalCoupling": {
      "symbol_id": "string",
      "valence": "float",
      "arousal": "float",
      "resonance_frequency": "float",
      "coupling_strength": "float"
    },
    "SceneGraph": {
      "objects": "object",
      "attributes": "object",
      "relationships": "array",
      "metadata": {
        "object_count": "integer",
        "relationship_count": "integer",
        "complexity": "float"
      }
    }
  },

  "integration_patterns": {
    "scene_decomposition": "image → objects → attributes → relationships",
    "value_assignment": "symbol → perception → emotion → value",
    "workspace_broadcast": "symbol → matriz_node → global_workspace → subscribers",
    "emotional_resonance": "symbol ↔ emotion (bidirectional coupling)"
  },

  "governance": {
    "requires_consent": ["scene_analysis", "emotional_processing"],
    "audit_level": "standard",
    "provenance_tracking": true,
    "interpretability": "high",
    "ethical_constraints": ["accurate_scene_representation", "preserve_emotional_context"]
  }
}