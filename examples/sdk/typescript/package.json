{
  "name": "@lukhas/sdk-examples",
  "version": "1.0.0",
  "description": "TypeScript SDK examples for LUKHAS AI API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "example:basic": "ts-node src/basic-client.ts",
    "example:streaming": "ts-node src/streaming-client.ts",
    "example:search": "ts-node src/search-client.ts",
    "example:dreams": "ts-node src/dreams-client.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "lukhas",
    "ai",
    "openai",
    "typescript",
    "sdk"
