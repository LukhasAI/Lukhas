[
  {
    "key": "cmd+;",
    "command": "github.copilot.chat.executeCommand",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+shift+;",
    "command": "github.copilot.chat.autoExecuteTerminalCommand",
    "when": "terminalFocus"
  },
  {
    "key": "tab",
    "command": "github.copilot.editor.action.acceptSuggestion",
    "when": "editorTextFocus && github.copilot.editor.hasSuggestion"
  },
  {
    "key": "cmd+tab",
    "command": "github.copilot.editor.action.acceptAllSuggestions",
    "when": "editorTextFocus && github.copilot.editor.hasSuggestion"
  },
  {
    "key": "cmd+shift+tab",
    "command": "github.copilot.editor.action.acceptAndExecute",
    "when": "editorTextFocus && github.copilot.editor.hasSuggestion"
  },
  {
    "key": "cmd+enter",
    "command": "github.copilot.chat.autoExecuteSelectedText",
    "when": "editorHasSelection"
  },
  {
    "key": "cmd+shift+enter",
    "command": "github.copilot.chat.autoOptimizeFile",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+alt+enter",
    "command": "github.copilot.chat.autoRefactorSelection",
    "when": "editorHasSelection"
  },
  {
    "key": "cmd+ctrl+enter",
    "command": "github.copilot.chat.autoGenerateTests",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+shift+ctrl+enter",
    "command": "github.copilot.chat.autoFixAllIssues",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+alt+t",
    "command": "github.copilot.terminal.executeAutonomousCommand",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+alt+f",
    "command": "github.copilot.workspace.autonomousFileManagement",
    "when": "explorerViewletFocus"
  },
  {
    "key": "cmd+alt+g",
    "command": "github.copilot.git.autonomousCommitAndPush",
    "when": "scmResourceGroup"
  },
  {
    "key": "cmd+alt+d",
    "command": "github.copilot.debug.autonomousDebugging",
    "when": "debuggersAvailable"
  },
  {
    "key": "cmd+alt+p",
    "command": "github.copilot.package.autonomousInstallation",
    "when": "editorTextFocus"
  }
]