{
 "$schema": "https://json.schemastore.org/sarif-2.1.0.json",
 "version": "2.1.0",
 "runs": [
  {
   "tool": {
    "driver": {
     "name": "gitleaks",
     "semanticVersion": "v8.0.0",
     "informationUri": "https://github.com/gitleaks/gitleaks",
     "rules": [
      {
       "id": "lukhas-api-key",
       "name": "LUKHAS API key detection",
       "shortDescription": {
        "text": "(?i)(lukhas[_-]?api[_-]?key|api[_-]?key)['\":\\s]*=?['\":\\s]*([a-z0-9_-]{20,})"
       }
      },
      {
       "id": "anthropic-api-key",
       "name": "Anthropic API key detection",
       "shortDescription": {
        "text": "(?i)(anthropic[_-]?api[_-]?key|claude[_-]?api[_-]?key)['\":\\s]*=?['\":\\s]*([a-z0-9_-]{20,})"
       }
      },
      {
       "id": "openai-api-key",
       "name": "OpenAI API key detection",
       "shortDescription": {
        "text": "(?i)(openai[_-]?api[_-]?key)['\":\\s]*=?['\":\\s]*([a-z0-9_-]{20,})"
       }
      },
      {
       "id": "huggingface-token",
       "name": "HuggingFace token detection",
       "shortDescription": {
        "text": "(?i)(hf[_-]?token|huggingface[_-]?token)['\":\\s]*=?['\":\\s]*([a-z0-9_-]{20,})"
       }
      },
      {
       "id": "guardian-system-keys",
       "name": "Guardian System internal keys",
       "shortDescription": {
        "text": "(?i)(guardian[_-]?key|safety[_-]?key|constitutional[_-]?key)['\":\\s]*=?['\":\\s]*([a-z0-9_-]{20,})"
       }
      },
      {
       "id": "database-credentials",
       "name": "Database connection strings and credentials",
       "shortDescription": {
        "text": "(?i)(database[_-]?url|db[_-]?url|connection[_-]?string)['\":\\s]*=?['\":\\s]*([a-z0-9_:/.-]{20,})"
       }
      },
      {
       "id": "jwt-secrets",
       "name": "JWT secret keys",
       "shortDescription": {
        "text": "(?i)(jwt[_-]?secret|token[_-]?secret)['\":\\s]*=?['\":\\s]*([a-z0-9_-]{20,})"
       }
      },
      {
       "id": "encryption-keys",
       "name": "Encryption keys and cryptographic material",
       "shortDescription": {
        "text": "(?i)(encryption[_-]?key|crypto[_-]?key|private[_-]?key)['\":\\s]*=?['\":\\s]*([a-z0-9_/+=]{20,})"
       }
      }
     ]
    }
   },
   "results": []
  }
 ]
}
