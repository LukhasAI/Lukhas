{
  "name": "lukhas-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:brand": "cd .. && node scripts/lint-brand.js",
    "lint:tone": "cd .. && node scripts/lint-tone.js",
    "lint:a11y": "cd .. && node scripts/lint-a11y.js",
    "lint:readability": "cd .. && node scripts/readability-scorer.js",
    "lint:poetic": "cd .. && node scripts/poetic-guard.js",
    "lint:vendor": "cd .. && node scripts/vendor-reference-linter.js",
    "lint:svg": "cd .. && node scripts/svg-freeze-validator.js",
    "lint:i18n": "cd .. && node scripts/international-false-friend-scanner.js",
    "vocab:validate": "cd .. && node scripts/vocab-validate.cjs",
    "vocab:suggest": "cd .. && node scripts/vocab-suggest.cjs",
    "vocab:check": "npm run vocab:validate && npm run vocab:suggest",
    "lint:policy": "npm run lint:brand && npm run lint:tone && npm run lint:a11y && npm run lint:readability && npm run lint:poetic && npm run lint:vendor && npm run lint:svg && npm run lint:i18n && npm run vocab:validate",
    "prebuild": "npm run lint:policy",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit --coverage",
    "test:integration": "jest --testPathPattern=integration --coverage",
    "test:security": "jest --testPathPattern=security --verbose",
    "test:api": "jest --testPathPattern=api --coverage",
    "test:performance": "jest --testPathPattern=performance --verbose",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:coverage": "jest --coverage --coverageReporters=text,lcov,html",
    "test:coverage:report": "jest --coverage && open coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false --maxWorkers=2",
    "test:all": "npm run test:ci && npm run test:e2e && npm run test:accessibility",
    "test:all:parallel": "concurrently \"npm run test:unit\" \"npm run test:integration\" \"npm run test:api\"",
    "test:phase6": "npm run test:unit && npm run test:integration && npm run test:security && npm run test:performance",
    "lighthouse": "lighthouse-ci autorun",
    "test:accessibility": "jest --testPathPattern=accessibility --verbose",
    "test:accessibility:ci": "lighthouse-ci autorun --config=.lighthouserc.json",
    "test:load": "artillery run tests/load/load-test.yml",
    "test:load:report": "artillery run tests/load/load-test.yml --output test-results/load-report.json",
    "test:security:audit": "npm audit && npm run test:security",
    "test:validate": "npm run lint && npm run test:phase6 && npm run test:e2e",
    "test:quality": "npm run test:coverage && npm run test:accessibility && npm run test:load",
    "policy:transparency": "node scripts/check-transparency.js",
    "policy:registries": "npm run lint:brand",
    "policy:brand": "npm run lint:brand",
    "policy:tone": "npm run lint:tone",
    "policy:tone-visibility": "node scripts/lint-tone-visibility.js",
    "policy": "npm run policy:transparency && npm run policy:registries && npm run policy:brand && npm run policy:tone && npm run policy:tone-visibility",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "test:auth": "jest packages/auth/__tests__",
    "test:rate": "jest packages/middleware/__tests__",
    "sdk:identity": "openapi-generator-cli generate -i proto_openapi/identity.yaml -g typescript-fetch -o packages/sdk-identity --additional-properties=npmName=@lukhas/sdk-identity",
    "sdk:wallet": "openapi-generator-cli generate -i proto_openapi/wallet.yaml -g typescript-fetch -o packages/sdk-wallet --additional-properties=npmName=@lukhas/sdk-wallet",
    "sdk:qrg": "openapi-generator-cli generate -i proto_openapi/qrg.yaml -g typescript-fetch -o packages/sdk-qrg --additional-properties=npmName=@lukhas/sdk-qrg",
    "sdk:consent": "openapi-generator-cli generate -i proto_openapi/consent.yaml -g typescript-fetch -o packages/sdk-consent --additional-properties=npmName=@lukhas/sdk-consent",
    "sdk:all": "npm run sdk:identity && npm run sdk:wallet && npm run sdk:qrg && npm run sdk:consent",
    "dev:apis": "ts-node --transpile-only dev-server.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@google-ai/generativelanguage": "^3.3.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^5.20.0",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-three/drei": "^9.92.0",
    "@react-three/fiber": "^8.15.0",
    "@tanstack/react-query": "^5.17.0",
    "@vercel/analytics": "^1.1.1",
    "argon2": "^0.31.2",
    "axios": "^1.11.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "critters": "^0.0.23",
    "express": "^4.19.2",
    "framer-motion": "^11.0.0",
    "glob": "^11.0.3",
    "gsap": "^3.12.4",
    "jose": "^6.0.12",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.303.0",
    "next": "^14.2.0",
    "openai": "^5.12.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.5.3",
    "react-textarea-autosize": "^8.5.9",
    "recharts": "^2.10.0",
    "shiki": "^0.14.7",
    "split-type": "^0.3.4",
    "tailwind-merge": "^2.2.0",
    "three": "^0.160.0",
    "ulid": "^2.3.0",
    "zod": "^3.22.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@lhci/cli": "^0.13.0",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@playwright/test": "^1.44.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/supertest": "^6.0.0",
    "@types/three": "^0.160.0",
    "artillery": "^2.0.9",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "msw": "^2.3.0",
    "nock": "^13.5.0",
    "postcss": "^8.4.32",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
