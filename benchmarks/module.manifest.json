{
  "schema_version": "1.0.0",
  "module": "benchmarks",
  "description": "LUKHAS benchmarks module implementing specialized benchmarks functionality with 19 components for integrated system operations.",
  "ownership": {
    "team": "Core",
    "codeowners": [
      "@lukhas-core"
    ]
  },
  "layout": {
    "code_layout": "package-root",
    "paths": {
      "code": ".",
      "config": "config",
      "tests": "tests",
      "docs": "docs",
      "assets": "assets"
    }
  },
  "runtime": {
    "language": "python",
    "entrypoints": [
      "benchmarks.matriz_pipeline.MATRIZBenchmarks",
      "benchmarks.memory_performance.MemoryBenchmarks",
      "benchmarks.memory_performance.run_all_benchmarks",
      "benchmarks.memory_performance.test_cascade_prevention_benchmark",
      "benchmarks.memory_performance.test_memory_recall_benchmark",
      "benchmarks.memory_system_benchmarks.MemoryItem",
      "benchmarks.memory_system_benchmarks.MemorySystemBenchmarks",
      "benchmarks.memory_system_benchmarks.MockMemorySystem"
    ]
  },
  "matrix": {
    "contract": "",
    "lane": "L2",
    "gates_profile": "standard"
  },
  "identity": {
    "requires_auth": false,
    "tiers": [],
    "scopes": []
  },
  "links": {
    "repo": "https://github.com/LukhasAI/Lukhas",
    "docs": "./docs/README.md",
    "issues": "https://github.com/LukhasAI/Lukhas/issues"
  },
  "tags": [
    "benchmarks"
  ],
  "observability": {
    "required_spans": [
      "lukhas.benchmarks.fold",
      "lukhas.benchmarks.operation",
      "lukhas.benchmarks.retrieval"
    ],
    "otel_semconv_version": "1.37.0"
  },
  "tokenization": {
    "enabled": false,
    "chain": "none"
  },
  "dependencies": [
    "memory"
  ],
  "contracts": []
}
