  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    },
    "headers": {
      "X-Trace-Id": {
        "description": "W3C trace context ID (32-char hex) for request correlation",
        "schema": {
          "type": "string",
          "pattern": "^[0-9a-f]{32}$"
        },
        "example": "4bf92f3577b34da6a3ce929d0e0e4736"
      },
      "X-Service-Version": {
        "description": "Deployment version (git SHA or version string)",
        "schema": {
          "type": "string"
        },
        "example": "a3f2d1c"
      },
      "X-RateLimit-Limit": {
        "description": "Max requests allowed in the current window",
        "schema": {
          "type": "integer",
          "minimum": 0
        }
      },
      "X-RateLimit-Remaining": {
        "description": "Requests remaining in the current window",
        "schema": {
          "type": "integer",
          "minimum": 0
        }
      },
      "X-RateLimit-Reset": {
        "description": "Epoch seconds until the current window resets",
        "schema": {
          "type": "integer",
          "format": "int64",
          "minimum": 0
        }
      },
      "X-RateLimit-Limit-Requests": {
        "description": "Maximum requests allowed in the current window (legacy)",
        "schema": {
          "type": "integer",
          "minimum": 1
        }
      },
      "X-RateLimit-Remaining-Requests": {
        "description": "Remaining requests in the current window (legacy)",
        "schema": {
          "type": "integer",
          "minimum": 0
        }
      },
      "X-RateLimit-Reset-Requests": {
        "description": "Seconds until the rate limit window resets (legacy)",
        "schema": {
          "type": "string"
        },
        "example": "42.150"
      },
      "Retry-After": {
        "description": "Seconds to wait before retrying (429 responses only)",
        "schema": {
          "type": "integer",
          "minimum": 1
        }
      }
    }
  }
}